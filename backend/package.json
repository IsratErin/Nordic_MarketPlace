{
  "name": "backend",
  "version": "1.0.0",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "build": "prisma generate && tsc",
    "vercel-build": "prisma generate && tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --ext ts,js,json --exec tsx src/server.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:unit": "node --experimental-vm-modules ./node_modules/jest/bin/jest --config jest.unit.config.js",
    "db:test:up": "docker compose --env-file .env.test -f docker-compose.test.yml up -d",
    "db:test:down": "docker compose --env-file .env.test -f docker-compose.test.yml down -v",
    "db:test:migrate": "cross-env NODE_ENV=test dotenv -e .env.test prisma migrate deploy",
    "test:int": "cross-env NODE_ENV=test dotenv -e .env.test -- node --experimental-vm-modules ./node_modules/jest/bin/jest --config jest.integration.config.js",
    "test:int:full": "npm run db:test:up && npm run db:test:migrate && npm run test:int && npm run db:test:down",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@types/cors": "^2.8.19",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "prisma": "^6.19.0",
    "winston": "^3.18.3",
    "zod": "^4.1.13",
    "dotenv": "^17.2.3"
  }
}
